{
  "name": "phambili-site",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "svg:clean": "node scripts/svg-clean.mjs",
    "svg:incoming:icons": "node scripts/svg-clean.mjs --in incoming/icons --outDir public/icons --mode icon --makeReact",
    "svg:incoming:art": "node scripts/svg-clean.mjs --in incoming/art --outDir public/illustrations --mode illustration --makeReact",
    "svg:one": "node scripts/svg-clean.mjs --in",
    "svg:opt-all": "svgo -f public --config=svgo.config.js",
    "svg:ringify": "node scripts/svg-ringify.mjs"
  },
  "dependencies": {
    "framer-motion": "^12.23.12",
    "next": "14.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@svgr/cli": "^8.1.0",
    "@svgr/core": "^8.1.0",
    "@svgr/webpack": "^8.1.0",
    "autoprefixer": "10.4.19",
    "jsdom": "^26.1.0",
    "postcss": "8.4.38",
    "svg-path-bounds": "^1.0.2",
    "svgo": "^4.0.0",
    "svgpath": "^2.6.0",
    "svgson": "^5.3.1",
    "tailwindcss": "3.4.7"
  }
}
